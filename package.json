{
  "name": "codebase-api-integrator",
  "version": "1.0.0",
  "description": "Analyze GitHub repositories and generate API integrations automatically",
  "main": "dist/codebaseApiIntegrator.js",
  "types": "dist/codebaseApiIntegrator.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node",
    "test": "ts-node testIntegrator.ts",
    "test:quick": "ts-node testIntegrator.ts --quick",
    "validate": "ts-node testIntegrator.ts --validate",
    "integrate": "ts-node integrateAndCommit.ts",
    "integrate:web": "ts-node integrateAndCommit.ts --web-apis",
    "config:validate": "ts-node configLoader.ts validate",
    "config:list": "ts-node configLoader.ts list",
    "clean": "rm -rf dist",
    "prepare": "npm run build"
  },
  "keywords": [
    "api",
    "integration",
    "code-generation",
    "openai",
    "typescript",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "openai": "^4.20.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.1",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}